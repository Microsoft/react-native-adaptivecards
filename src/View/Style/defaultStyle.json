    "action": {
        "actionSet": {
            "direction": "row",
            "spacing": 10
        },
        "button": {
            "borderRadius": 4,
            "backgroundColor": "#277BDF",
            "spacing": 10,
            "paddingVertical": 10,
            "paddingHorizontal": 10,
            "textColor": "#FFF",
            "fontSize": 13
        }
    }
